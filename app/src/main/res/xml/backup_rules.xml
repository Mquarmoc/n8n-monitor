<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Only backup settings, not sensitive data -->
    <include domain="sharedpref" path="settings"/>
    <!-- Include domains first, then exclude sensitive data -->
    <include domain="database"/>
    <include domain="file"/>
    <exclude domain="database" path="n8n_monitor.db"/>
    <exclude domain="file" path="logs"/>
</full-backup-content>